import java.util.ArrayList;

public class TwoLists {
   
    private static ArrayList<Integer> smallerThan = new ArrayList<Integer>();
    private static ArrayList<Integer> biggerThan = new ArrayList<Integer>();;
  
    public TwoLists(ArrayList<Integer> a, ArrayList<Integer> b){

        for(int i=0; i < a.size(); i++){
            smallerThan.add(a.get(i)); 
        }

        for(int i=0; i < b.size(); i++){
            biggerThan.add(b.get(i)); 
        }

    }
}