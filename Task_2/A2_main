		Patienten.addPerson(p1);
		
		System.out.println(Patienten.getNextPerson().getName() + Patienten.getNextPerson().getPrio());
		System.out.println(Patienten.getNextPerson().getName() + Patienten.getNextPerson().getPrio());
		System.out.println(Patienten.getNextPerson().getName() + Patienten.getNextPerson().getPrio());
		System.out.println(Patienten.getNextPerson().getName() + Patienten.getNextPerson().getPrio());
